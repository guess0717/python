package com.anna; 

public class Person {
	float weight;
	float height;
	float BMI = weight/(height*height);

	public void sayhello(){
		System.out.println("Hello World");
		
	}

	public static void main(String[] args){
		Person p = new Person();
		p.sayhello();
		p.weight = 45.8f;
		p.height = 1.56f;
		System.out.println(BMI);
		
		
		
		float w = 46.2f ;
		float h = 1.56f ; // 公尺
		float bmi = w / (h*h);
		System.out.println(bmi);
		
		
	}

}
